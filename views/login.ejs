<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>land</title>
    <script type="text/javascript" src="../js/jQuery.js"></script>
    <link rel="stylesheet" type="text/css" href="../css/sign.css">
</head>
<body>
    <div id="wrapper">
        <div id="login" class="animate form">
            <form >
                <h1>
                    Log in
                </h1>
                <p>
                    <label for="username" class="uname" >
                        Your username
                    </label>
                    <input type="text" name="username" id="username" required="required" placeholder="myusername">
                </p>
                <p>
                    <label for="password" class="youpassword" >
                        Your password
                    </label>
                    <input type="text" name="password" id="password" required="required" placeholder="password">
                </p>
                <p class="keeplogin">
                    <input type="checkbox" name="loginkeeping" id="loginkeeping" value="loginkeeping">
                    <label for="loginkeeping">Keep me logged in</label>
                    <p class="login button">
                        <input type="submit" value="Login" class="btn" id="jump">
                    </p>
                </p>
            </form>
        </div>
    </div>
    <script type="text/javascript">
        $(function(){
            $("#jump").click(function () {
                var username = $("#username").val();
                var password = $("#password").val();
                if($.trim(username)==""){
                    alert("name is error!");
                    return false;
                }
                if($.trim(password)==""){
                    alert("username is error!");
                    return false;
                }
                window.open("./home.html");
            })
        })


    </script>
</body>
</html>